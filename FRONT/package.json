{
	"name": "vueproject",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve --open",
		"build": "vue-cli-service build",
		"test": "jest",
		"lint": "vue-cli-service lint",
		"test:unit": "vue-cli-service test:unit"
	},
	"dependencies": {
		"@vue/babel-preset-app": "3.6.0",
		"@vue/test-utils": "1.0.0-beta.29",
		"autoprefixer": "9.5.1",
		"driver.js": "0.9.7",
		"echarts": "4.2.1",
		"element-ui": "2.12.0",
		"font-awesome": "4.7.0",
		"html2canvas": "1.0.0-rc.3",
		"jquery": "3.4.1",
		"jspdf": "1.5.3",
		"less": "3.10.3",
		"less-loader": "5.0.0",
		"lodash": "4.17.15",
		"mockjs": "1.0.1-beta3",
		"size-sensor": "0.2.2",
		"spark-md5": "3.0.0",
		"vue": "2.6.10",
		"vue-awesome-swiper": "3.1.3",
		"vue-draggable-resizable-gorkys": "2.2.0",
		"vue-router": "3.1.3",
		"vue-simple-uploader": "0.7.1",
		"vuex": "3.1.1"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "3.4.0",
		"@vue/cli-plugin-eslint": "3.4.0",
		"@vue/cli-plugin-unit-jest": "^3.11.0",
		"@vue/cli-service": "3.4.0",
		"@vue/test-utils": "1.0.0-beta.29",
		"axios": "0.19.0",
		"babel-core": "7.0.0-bridge.0",
		"babel-jest": "^23.6.0",
		"babel-polyfill": "6.26.0",
		"css-loader": "1.0.1",
		"style-loader": "0.23.1",
		"vue-template-compiler": "2.6.10",
		"worker-loader": "2.0.0"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"rules": {
			"no-console": 0,
			"no-unused-vars": "off",
			"no-undef": 0,
			"no-mixed-spaces-and-tabs": 0,
			"no-extra-boolean-cast": 0,
			"vue/require-v-for-key": 0,
			"vue/valid-v-on": 0
		},
		"parserOptions": {
			"parser": "babel-eslint"
		},
		"overrides": [{
			"files": [
				"**/__tests__/*.{j,t}s?(x)"
			],
			"env": {
				"jest": true
			}
		}]
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"defaults"
	]
}